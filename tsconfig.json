{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
   "baseUrl": ".",
    "paths": {
      "@component/*": ["./components/*"],
      "@hooks/*": ["./hooks/*"],
      "@apis/*": ["./apis/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
