{
  "expo": {
    "name": "smartdownloader",
    "slug": "smartdownloader",
    "description": "Downloader video from your favorite social media platform",
    "owner": "codeauthor",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "notification": {
      "icon": "./assets/images/notification_Icon.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    },
    "icon": "./assets/images/smartDownloaderLogo.png",
    "scheme": "com.smartdownloader.codeauthor",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartdownloader.codeauthor"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "package": "com.smartdownloader.codeauthor"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.ico"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
